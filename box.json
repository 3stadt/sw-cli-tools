{
    "alias": "sw.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": ["src", "assets"],
    "files": [
        "LICENSE",
        "README.md",
        "config.yaml.dist",
        "vendor/autoload.php",
        "vendor/herrera-io/phar-update/res/schema.json",
        "vendor/shopwarelabs/plugin-info/res/plugin-info-schema.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["tests"],
            "in": [
                "vendor/composer",
                "vendor/shopwarelabs",
                "vendor/justinrainbow",
                "vendor/symfony/console",
                "vendor/symfony/config",
                "vendor/symfony/dependency-injection",
                "vendor/symfony/process",
                "vendor/symfony/filesystem",
                "vendor/symfony/yaml",
                "vendor/seld/jsonlint",
                "vendor/kherge/amend",
                "vendor/justinrainbow/json-schema",
                "vendor/herrera-io/json",
                "vendor/herrera-io/phar-update",
                "vendor/herrera-io/version",
                "vendor/dnoegel",
                "vendor/ramsey",
                "src"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin/sw",
    "output": "sw.phar",
    "stub": true
}
