{
    "alias": "sw.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Json",
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": ["src", "assets"],
    "files": [
        "LICENSE",
        "README.md",
        "config.yaml.dist",
        "vendor/autoload.php",
        "vendor/herrera-io/phar-update/res/schema.json",
        "vendor/shopwarelabs/plugin-info/res/plugin-info-schema.json"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["tests"],
            "in": [
                "vendor",
                "src"
            ]
        }
    ],
    "git-version": "package_version",
    "main": "bin/sw",
    "output": "sw.phar",
    "stub": true
}
